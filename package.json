{
  "name": "graphql-contacts-sync",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "webpack": "webpack --config webpack.config.js --watch",
    "start:dev:graphql-auth": "export APP_INPUT=graphql-auth && nest start graphql-auth --watch",
    "start:dev:mongodb-consumer": "export APP_INPUT=mongodb-consumer && nest start mongodb-consumer",
    "start:dev:mysql-consumer": "export APP_INPUT=mysql-consumer && nest start mysql-consumer",
    "start:dev:all": "concurrently \"npm run start:dev:graphql-auth\" \"npm run start:dev:mongodb-consumer\" \"npm run start:dev:mysql-consumer\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/graphql-contacts-sync/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/graphql-contacts-sync/test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./apps/mysql-consumer/src/config/typeorm.ts",
    "migration:generate": "npm run typeorm -- -d ./apps/mysql-consumer/src/config/typeorm.ts migration:generate ./src/migrations/$npm_config_name",
    "migration:create": "npm run typeorm -- migration:create ./apps/mysql-consumer/src/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d ./apps/mysql-consumer/src/config/typeorm.ts migration:revert"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@golevelup/nestjs-rabbitmq": "^4.1.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.1",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "dotenv": "^16.4.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "graphql": "^16.8.1",
    "graphql-constraint-directive": "^5.3.0",
    "graphql-tools": "^9.0.0",
    "mongoose": "^8.1.1",
    "mysql2": "^3.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/graphql-auth",
      "<rootDir>/apps/mongodb-consumer",
      "<rootDir>/apps/mysql-consumer"
    ],
    "modulePathIgnorePatterns": [
      ".*\\.module\\.ts$",
      ".*\\main\\.ts$",
      ".*\\.e2e-spec\\.ts$",
      "depedencie\\.ts$",
      "provider\\.ts$"
    ],
    "coveragePathIgnorePatterns": [
      "migrations",
      "config",
      "validators",
      "depedencies"
    ],
    "moduleNameMapper": {
      "^@graphql-auth/src/(.*)$": "<rootDir>/apps/graphql-auth/src/$1",
      "^@mongodb-consumer/src/(.*)$": "<rootDir>/apps/mongodb-consumer/src/$1",
      "^@mysql-consumer/src/(.*)$": "<rootDir>/apps/mysql-consumer/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
